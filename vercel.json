{
  "version": 2,
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/api/date", "dest": "/api/date.py" },
    { "src": "/api/ip/(.*)", "dest": "/api/ip_query.py" },
    { "src": "/api/(.*)", "dest": "/api/web.py" }, 
    { "src": "/(.*)", "status": 404, "dest": "/404.html" }
    
  ]
}
